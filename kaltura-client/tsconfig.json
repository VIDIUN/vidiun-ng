{
  "compilerOptions": {
    "baseUrl": "src",
    "outDir": "./.tmp",
    "declaration": true,
    "stripInternal": true,
    "experimentalDecorators": true,
    "strictNullChecks": false,
    "noImplicitAny": false,
    "noEmitHelpers": false,
    "module": "es2015",
    "importHelpers": false,
    "moduleResolution": "node",
    "sourceMap": true,
    "inlineSources": true,
    "target": "es5",
    "skipLibCheck": true,
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2015",
      "dom"
    ],
    "paths": {
      "@angular/core": [
        "../node_modules/@angular/core"
      ],
      "@angular/core/*": [
        "../node_modules/@angular/core/*"
      ],
      "@angular/http": [
        "../node_modules/@angular/http"
      ],
      "@angular/http/*": [
        "../node_modules/@angular/http/*"
      ],
      "kaltura-typescript-client": [
        "../node_modules/kaltura-typescript-client"
      ],
      "kaltura-typescript-client/*": [
        "../node_modules/kaltura-typescript-client/*"
      ],
      "rxjs": [
        "../node_modules/rxjs"
      ],
      "rxjs/*": [
        "../node_modules/rxjs/*"
      ],
      "tslib": [
        "../node_modules/tslib"
      ],
      "tslib/*": [
        "../node_modules/tslib/*"
      ]
    }
  },
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "src/**/*.spec.ts"
  ],
  "angularCompilerOptions": {
    "skipTemplateCodegen": true
  }
}