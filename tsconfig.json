{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "importHelpers": true,
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2017",
      "dom"
    ],
    "paths": {
      "@vidiun-ng/vidiun-common": [
        "dist/vidiun-ng/vidiun-common"
      ],
      "@vidiun-ng/vidiun-common/*": [
        "dist/vidiun-ng/vidiun-common/*"
      ],
      "@vidiun-ng/vidiun-ui": [
        "dist/vidiun-ng/vidiun-ui"
      ],
      "@vidiun-ng/vidiun-ui/*": [
        "dist/vidiun-ng/vidiun-ui/*"
      ],
      "@vidiun-ng/vidiun-logger": [
        "dist/vidiun-ng/vidiun-logger"
      ],
      "@vidiun-ng/vidiun-logger/*": [
        "dist/vidiun-ng/vidiun-logger/*"
      ],
      "@vidiun-ng/vidiun-primeng-ui": [
        "dist/vidiun-ng/vidiun-primeng-ui"
      ],
      "@vidiun-ng/vidiun-primeng-ui/*": [
        "dist/vidiun-ng/vidiun-primeng-ui/*"
      ],
      "@vidiun-ng/mc-shared": [
        "dist/vidiun-ng/mc-shared"
      ],
      "@vidiun-ng/mc-shared/*": [
        "dist/vidiun-ng/mc-shared/*"
      ]
    }
  }
}